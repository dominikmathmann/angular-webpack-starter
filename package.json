{
  "name": "angular-webpack-starter",
  "version": "1.0.0",
  "keywords": [
    "angular2",
    "angular",
    "webpack",
    "starter",
    "json-server",
    "protractor"
  ],
  "scripts": {
    "dev-server": "webpack-dev-server --hot --inline --config ./webpack.dev.config.js",
    "start": "npm run dev-server",
    "prod": "webpack --config ./webpack.prod.config.js",
    "start-prod": "webpack-dev-server --port 4300 --config ./webpack.prod.config.js",
    "protractor": "rimraf ./test/target && tsc ./test/**/*.ts -t ES5 --outDir ./test/target && protractor test/protractor.conf.js",
    "test": "concurrently --kill-others 'npm run start-prod' 'node ./test/delayed-protractor.js'",
    "test-f": "nodemon -e ts,html,css --exec 'npm run test'"
  },
  "author": "Dominik Mathmann, GEDOPLAN",
  "contributors": [],
  "watch-test": {
    "test": {
      "patterns": [
        "src",
        "test"
      ],
      "extensions": "js,jsx,ts,css,html",
      "quiet": true
    }
  },
  "dependencies": {
    "@angular/common": "~2.0.1",
    "@angular/compiler": "~2.0.1",
    "@angular/core": "~2.0.1",
    "@angular/forms": "~2.0.1",
    "@angular/http": "~2.0.1",
    "@angular/platform-browser": "~2.0.1",
    "@angular/platform-browser-dynamic": "~2.0.1",
    "@angular/router": "~3.0.1",
    "@angular/upgrade": "~2.0.1",
    "angular-in-memory-web-api": "~0.1.1",
    "angular2-busy": "^1.0.1",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "font-awesome": "^4.6.3",
    "jquery": "^3.1.0",
    "ng2-toasty": "^2.1.0",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "ts-loader": "^0.8.2",
    "web-animations-js": "^2.2.2",
    "webpack-merge": "^0.14.1",
    "zone.js": "^0.6.25"
  },
  "devDependencies": {
    "concurrently": "^3.0.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.22.0",
    "jasmine-core": "^2.4.1",
    "jasmine-spec-reporter": "^2.5.0",
    "nodemon": "^1.10.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "script-loader": "^0.7.0",
    "style-loader": "^0.13.1",
    "typescript": "^2.0.3",
    "typescript-loader": "^1.1.3",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
